- if flash[:notice]
  .flash= flash[:notice]
<h3>Contact Records</h3>
= "Contact Records: #{@contact_records.length}"
%br
%ul
  - @contact_records.each do |contact_record|
    %ul= link_to "#{contact_record.created_at.strftime("%m/%d/%y:%I%M")} - #{contact_record.company}", contact_record_path(contact_record.id)

%br
=link_to 'New Contact Record', new_contact_record_path

